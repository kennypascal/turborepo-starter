{
    "name": "turborepo-starter",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "test": "turbo run test",
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "clean": "turbo run clean",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json,cjs}\""
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "eslint": "^8.28.0",
        "eslint-config-custom": "*",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "prettier": "^2.8.7",
        "prettier-plugin-tailwindcss": "^0.2.7",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "packageManager": "npm@7.21.1"
}
